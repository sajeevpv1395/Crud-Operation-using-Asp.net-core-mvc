@model IEnumerable<Carrier>
@if(TempData["success"]!=null)
{
    <h2>@TempData["success"]</h2>
}



<div >
    <nav class="navbar navbar-light bg-light">
  <div class="container-fluid">
    <a class="navbar-brand">Carrier Page</a>
    <form class="d-flex">
    
     <a asp-action="Create" asp-controller="Carrier"  class="btn btn-success" ><i class="bi bi-plus-circle"></i> &nbsp;Create new Category</a>

    </form>
  </div>
</nav>
</div>
  
   <br />
   <br />
<table class="table table-bordered table-striped" style="width:100%"/>
    <thead>
        <tr>
            <th>
               First Name
			</th>
            <th>
                Last Name
			</th>
            <th>
                Phone Number 
            </th>
               <th>
                Email
            </th>
               <th>
                Qualification
            </th>
		</tr>
	</thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="16%">
                    @obj.FirstName
				</td>
                <td width="16%">
                    @obj.LastName
				</td>
                <td width="16%">
                    @obj.PhoneNumber
				</td>
                  <td width="16%">
                    @obj.Email
				</td>
                  <td width="16%">
                    @obj.Qualification
				</td>
                     <td width="30%" style="display:inline" >
  <a asp-controller="Carrier" asp-action="Edit" asp-route-id="@obj.Id"
                        class="btn btn-primary mx-2"> <i class="bi bi-pencil-square"></i> Edit</a>
                        <a asp-controller="Carrier" asp-action="Delete" asp-route-id="@obj.Id"
                        class="btn btn-danger mx-2" > <i class="bi bi-trash-fill"></i> Delete</a>
                   
				</td>
			
			</tr>
        }
    </tbody>
  